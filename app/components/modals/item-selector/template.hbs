<div class="row center spaceBetween uiContainer">
  {{title}}
  {{ui/icon-button
    class='cancel'
    flat=true
    color=colors.DARK_GREY
    type='close'
    click=(action 'foo')}}
</div>
{{#power-select
    selected=currentItem
    options=model
    placeholder=placeholder
    searchEnabled=true
    searchField='label'
    onchange=(action 'onItemSelected')
    as |location|}}
  {{location.label}}
{{/power-select}}
