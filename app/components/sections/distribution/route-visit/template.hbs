{{#ui/dd-card colorScheme=colorScheme as |card|}}
  {{card.title-bar
      title=(concat model.id "." "YU001")
      subTitle=company.name
      index=index
      actionIcon="clear"
      action=(if (not model.isOrphan) (action "removeRouteVisit"))}}

  {{card.body
    content=locations.firstObject.name
    infoIcons=infoIcons
    colorScheme=colorScheme}}
{{/ui/dd-card}}
