{{#ui/dd-card colorScheme=colorScheme as |card|}}
  {{card.title-bar
      title=location.code
      subTitle=company.name
      index=index
      actionIcon="clear"
      action=(if (not model.isOrphan) (action "removeRouteVisit"))}}

  {{card.body
      content=location.name
      infoIcons=infoIcons
      colorScheme=colorScheme}}
{{/ui/dd-card}}
