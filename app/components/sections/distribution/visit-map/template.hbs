{{#leaflet-map
    lat=lat
    lng=lng
    zoom=zoom as |layers|}}

  {{layers.tile url="http://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}@2x.png"}}

  {{#each openRouteVisits as |rv|}}
    {{sections/distribution/map-pin model=rv}}
  {{/each}}

  {{#each handledRouteVisitsNormalized as |data|}}
    {{sections/distribution/map-pin model=data.rv index=data.index}}
  {{/each}}

{{/leaflet-map}}
