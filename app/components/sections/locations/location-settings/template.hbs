<input
  type="text"
  name="name"
  class="name"
  placeholder="Location nickname"
  value="{{model.name}}"
  oninput={{action 'fieldChanged' 'name'}}
  onchange={{action 'fieldChanged' 'name'}}
  onblur={{action save}}>

<input
  type="text"
  name="deliveryRate"
  class="deliveryRate"
  placeholder='Delivery rate'
  value="{{model.deliveryRate}}"
  oninput={{action 'fieldChanged' 'deliveryRate'}}
  onchange={{action 'fieldChanged' 'deliveryRate'}}
  onblur={{action save}}>
